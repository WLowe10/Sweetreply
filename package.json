		"https-proxy-agent": "^7.0.4",
		"isbot": "^4.1.0",
		"jsonwebtoken": "^9.0.2",
		"nanoid": "^3.3.6",
		"node-html-parser": "^6.1.13",
		"openai": "^4.33.0",
		"p-limit": "^3.1.0",
