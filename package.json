{
	"name": "sweetreply",
	"private": true,
	"sideEffects": false,
	"type": "module",
	"scripts": {
		"start": "NODE_ENV=production node builds/server/main.js",
		"dev": "tsx watch --clear-screen=false --ignore '.cache/**' --ignore 'app/**' --ignore 'vite.config.ts.timestamp-*' --ignore 'builds/**' --ignore 'node_modules/**' --env-file .env ./server/main.ts",
		"build": "npm run build:app && npm run build:server",
		"build:app": "remix vite:build",
		"build:server": "tsup",
		"install-chrome": "pnpm dlx puppeteer browsers install chrome@125.0.6422.60",
		"email:preview": "tsx watch ./scripts/email/preview.ts",
		"db:generate": "prisma generate",
		"db:migrate-create": "prisma migrate dev --create-only",
		"db:migrate-dev": "prisma migrate dev",
		"db:migrate": "prisma migrate deploy",
		"db:push": "prisma db push",
		"db:genpush": "pnpm db:generate && pnpm db:push",
		"format": "prettier --write \"**/*.{ts,tsx,md}\"",
		"clean": "pnpm clean:out && pnpm clean:modules && pnpm clean:dist",
		"clean:modules": "rm -rf node_modules",
		"clean:out": "rm -rf out",
		"clean:dist": "rm -rf dist",
		"clean:generated": "rm -rf zod dbml",
		"typecheck": "tsc --noEmit"
	},
	"dependencies": {
		"@aws-sdk/client-ses": "^3.535.0",
		"@faire/mjml-react": "^3.3.0",
		"@hookform/resolvers": "^3.3.4",
		"@mantine/charts": "^7.6.2",
		"@mantine/core": "^7.6.2",
		"@mantine/dates": "^7.8.0",
		"@mantine/hooks": "^7.6.2",
		"@mantine/notifications": "^7.8.0",
		"@prisma/client": "^5.10.2",
		"@remix-run/node": "^2.8.1",
		"@remix-run/react": "^2.8.1",
		"@remix-run/serve": "^2.8.1",
		"@tabler/icons-react": "^3.1.0",
		"@tanstack/react-query": "4",
		"@tanstack/react-query-devtools": "4",
		"@trpc/client": "^10.45.1",
		"@trpc/react-query": "^10.45.1",
		"@trpc/server": "^10.45.1",
		"argon2": "^0.40.1",
		"axios": "^1.6.8",
		"bull": "^4.12.2",
