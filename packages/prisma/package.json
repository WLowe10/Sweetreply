{
    "name": "@replyon/prisma",
    "version": "0.0.0",
    "private": true,
    "main": "./index.ts",
    "types": "./index.ts",
    "files": [
        "zod"
    ],
    "scripts": {
        "db:studio": "prisma studio",
        "db:generate": "prisma generate",
        "db:push": "prisma db push",
        "db:nuke": "prisma migrate reset && prisma db push && yarn seed",
        "db:seed": "prisma generate && ts-node --project tsconfig.json --compiler-options {\\\"module\\\":\\\"CommonJS\\\"} prisma/newSeed.ts",
        "db:remove-seed": "prisma generate && ts-node --project tsconfig.json --compiler-options {\\\"module\\\":\\\"CommonJS\\\"} prisma/removeSeed.ts",
        "postinstall": "pnpm db:generate"
    },
    "dependencies": {
        "@prisma/client": "^5.10.2",
        "zod": "^3.21.4"
    },
    "devDependencies": {
        "@replyon/tsconfig": "workspace:*",
        "prisma": "^5.10.2",
        "prisma-dbml-generator": "^0.12.0",
        "zod-prisma": "^0.5.4"
    }
}
