{
    "$schema": "https://json.schemastore.org/tsconfig",
    "display": "Next.js",
    "extends": "./base.json",
    "compilerOptions": {
        "target": "es5",
        "lib": ["dom", "dom.iterable", "esnext"],
        "isolatedModules": true,
        "allowJs": true,
        "skipLibCheck": true,
        "noEmit": true,
        "incremental": true,
        "esModuleInterop": true,
        "module": "esnext",
        "jsx": "preserve",
        "resolveJsonModule": true
    },
    "include": ["src", "next-env.d.ts"],
    "exclude": ["node_modules"]
}
